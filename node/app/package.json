{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --name=app --target=lib src/main.ts",
    "build:vac": "vue-cli-service build --name=vac --target=lib --dest=../../public/web/themes/custom/tbdtheme/assets/dist-vac src/main-vac.ts",
    "lint": "vue-cli-service lint",
    "develop": "yarn build --watch --mode=development",
    "develop:vac": "yarn build:vac --watch --mode=development",
    "dev:wc": "vue-cli-service build --mode=development --watch --target=wc",
    "build:wc": "vue-cli-service build --target=wc",
    "dev:wc:FilterChartWrapper": "yarn dev:wc --dest ../../public/web/themes/custom/tbdtheme/assets/dist-webcomponents/filter-chart-wrapper src/webcomponents/FilterChartWrapper.vue",
    "build:wc:FilterChartWrapper": "yarn build:wc --dest ../../public/web/themes/custom/tbdtheme/assets/dist-webcomponents/filter-chart-wrapper src/webcomponents/FilterChartWrapper.vue"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "axios": "0.21.1",
    "core-js": "^3.6.5",
    "html2canvas": "^1.4.1",
    "json-to-csv-export": "^2.1.0",
    "lodash": "^4.17.21",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-grid-layout": "^2.3.12",
    "vue-popperjs": "^2.3.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-select": "^3.16.0",
    "vue-slider-component": "^3.2.15",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-template-compiler": "^2.6.11"
  }
