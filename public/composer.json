{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "enyo/dropzone": {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "v5.5.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dropzone/dropzone/archive/refs/tags/v5.5.0.zip",
                    "type": "zip"
                }
            }
        },
        "mozilla/pdf.js": {
            "type": "package",
            "package": {
                "name": "mozilla/pdf.js",
                "version": "v2.4.456",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mozilla/pdf.js/releases/download/v2.4.456/pdfjs-2.4.456-es5-dist.zip",
                    "type": "zip"
                }
            }
        },
        "kenwheeler/slick": {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/v1.8.1.zip",
                    "type": "zip"
                }
            }
        },
        "furf/jquery-ui-touch-punch": {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        "almende/vis": {
            "type": "package",
            "package": {
                "name": "almende/vis",
                "version": "v4.21.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/almende/vis/archive/refs/tags/v4.21.0.zip",
                    "type": "zip"
                }
            }
        },
        "maphilight": {
            "type": "package",
            "package": {
              "name": "kemayo/maphilight",
              "version": "v1.4.0",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/kemayo/maphilight/archive/master.zip",
                "type": "zip"
              }
            }
          },
          "raphael": {
            "type": "package",
            "package": {
              "name": "dmitrybaranovskiy/raphael",
              "version": "v2.3.0",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/dmitrybaranovskiy/raphael/archive/master.zip",
                "type": "zip"
              }
            }
          },
          "icheck": {
            "type": "package",
            "package": {
              "name": "dargullin/icheck",
              "version": "v1.0.3",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/dargullin/icheck/archive/master.zip",
                "type": "zip"
              }
            }
          }
    },
    "require": {
        "dargullin/icheck": "^1.0.3",
        "dmitrybaranovskiy/raphael": "^2.3.0",
        "kemayo/maphilight": "^1.4.0",
        "wikimedia/composer-merge-plugin": "2.0",
        "composer/installers": "v1.12.0",
        "cweagans/composer-patches": "1.7.1",
        "opigno/opigno_lms": "3.1.0",
        "drupal/block_visibility_conditions": "^1.0",
        "drupal/bootstrap5": "^1.1",
        "drupal/calendar": "1.0.0-alpha4",
        "drupal/calendar_datetime": "1.0.0-alpha4",
        "drupal/ckeditor_config": "^3.1",
        "drupal/color_field": "^3.0",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "~9.5.0",
        "drupal/core-project-message": "~9.5.0",
        "drupal/core-recommended": "~9.5.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/facets": "^2.0",
        "drupal/h5p": "2.0.0-alpha2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/media_entity_browser": "2.0.0-alpha3",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/recaptcha_v3": "^1.6",
        "drupal/redirect": "^1.7",
        "drupal/search_api": "^1.20",
        "drupal/search_api_solr": "^4.2",
        "drupal/sendgrid_integration": "^1.7",
        "drupal/simple_sitemap": "^4.1",
        "drupal/taxonomy_term_depth": "^2.5",
        "drupal/twig_tweak": "^3.1",
        "drupal/video": "1.5.0-alpha1",
        "drush/drush": "^10.3",
        "npm-asset/jquery-simple-color": "^1.2.1",
        "npm-asset/spectrum-colorpicker": "^1.8",
        "rusticisoftware/tincan": "~1.1.1",
        "drupal/mysql56": "^1.3",
        "drupal/devel": "^4.1",
        "drupal/module_missing_message_fixer": "^2.0",
        "drupal/theme_switcher": "^1.2",
        "drupal/flood_control": "^2.2",
        "drupal/autologout": "^1.4",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/admin_toolbar": "^3.3",
        "drupal/pathauto": "^1.11",
        "drupal/paragraphs": "^1.15",
        "drupal/google_tag": "^1.4",
        "drupal/metatag": "^2.0",
        "drupal/login_destination": "^2.0@beta"
    },
    "require-dev": {
        "drush/drush": "^10",
        "drupal/console": "~1.0"
    },
    "replace": {
        "h5p/h5p-core": "*",
        "h5p/h5p-editor": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": false,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/console-extend-plugin": true,
            "topfloor/composer-cleanup-vcs-dirs": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/spectrum": [
                "npm-asset/spectrum-colorpicker"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": false,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
